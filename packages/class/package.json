      }
    },
    "./cli": {
      "import": {
        "default": "./dist/cli.js",
        "types": "./dist/cli.d.ts"
      }
    },
    "./config": {
      "import": {
        "default": "./dist/config.js",
        "types": "./dist/config.d.ts"
      }
