{
  "name": "@classmodel/class",
  "type": "module",
  "version": "0.0.7",
  "exports": {
    "./package.json": "./package.json",
    "./class": {
      "import": {
        "default": "./dist/class.js",
        "types": "./dist/class.d.ts"
      }
    },
    "./cli": {
      "import": {
        "default": "./dist/cli.js",
        "types": "./dist/cli.d.ts"
      }
    },
    "./config": {
      "import": {
        "default": "./dist/config.js",
        "types": "./dist/config.d.ts"
      }
    },
    "./config.json": "./src/config.json",
    "./runner": {
      "import": {
        "default": "./dist/runner.js",
        "types": "./dist/runner.d.ts"
      }
    },
    "./bmi": {
      "import": {
        "default": "./dist/bmi.js",
        "types": "./dist/bmi.d.ts"
      }
    },
    "./validate": {
      "import": {
        "default": "./dist/validate.js",
        "types": "./dist/validate.d.ts"
      }
    }
  },
  "homepage": "https://classmodel.github.io/class-web",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/classmodel/class-web.git"
  },
  "bugs": {
    "url": "https://github.com/classmodel/class-web/issues"
  },
  "files": ["dist", "src/config.json", "README.md", "LICENSE"],
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepack": "pnpm build",
    "test": "tsx --test src/*.test.ts",
    "typecheck": "tsc --noEmit",
    "json2ts": "json2ts src/config.json src/config.ts",
    "docs": "typedoc",
    "check-exports": "attw --pack ."
  },
  "main": "./dist/class.js",
  "bin": "./dist/cli.js",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@types/node": "^20.13.1",
    "json-schema-to-typescript": "^15.0.2",
    "tsx": "^4.16.5",
    "typedoc": "^0.26.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "ajv": "^8.17.1",
    "commander": "^12.1.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}
