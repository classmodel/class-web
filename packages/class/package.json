    "./output": {
      "import": {
        "default": "./dist/output.js",
        "types": "./dist/output.d.ts"
      }
    },
    "./profiles": {
      "import": {
        "default": "./dist/profiles.js",
        "types": "./dist/profiles.d.ts"
      }
